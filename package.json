{
   "name": "chat",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "dependencies": {
      "body-parser": "^1.19.0",
      "dotenv": "^8.1.0",
      "express": "^4.17.1",
      "express-serve-static-core": "^0.1.1",
      "pg": "^7.12.1",
      "socket.io": "^2.2.0",
      "typeorm": "0.2.18",
      "uuid": "^3.3.3"
   },
   "devDependencies": {
      "@types/express": "^4.17.1",
      "@types/http-server": "^0.10.0",
      "@types/jest": "^24.0.18",
      "@types/node": "^8.10.54",
      "@types/pg": "^7.11.1",
      "@types/request": "^2.48.3",
      "@types/socket.io": "^2.1.3",
      "@types/uniqid": "^4.1.3",
      "@types/uuid": "^3.4.5",
      "jest": "^24.9.0",
      "pg-native": "^3.0.0",
      "through": "^2.3.8",
      "ts-jest": "^24.1.0",
      "ts-loader": "^6.1.0",
      "ts-node": "^3.3.0",
      "tsconfig-paths": "^3.9.0",
      "tslint": "^5.19.0",
      "tslint-config-airbnb": "^5.11.1",
      "types.d.ts": "^0.4.9",
      "typescript": "^3.4.3",
      "uniqid": "^5.0.3",
      "webpack-cli": "^3.3.9"
   },
   "scripts": {
      "test:unit": "jest --forceExit --verbose --passWithNoTests --coverage",
      "start": "dotenv ts-node src/app.ts",
      "database": "dotenv ts-node src/postgres.ts",
      "typeorm": "ts-node --project tsconfig.json ./node_modules/.bin/typeorm migration:generate -n init_migration -f ormconfig.json",
      "migration:create": "sudo typeorm migration:create -n newMigrations",
      "migration:run": "ts-node --project tsconfig.json ./node_modules/.bin/typeorm migration:run -f ormconfig.json",
      "migration:revert": "sudo typeorm migration:revert",
      "migration:generate": "sudo ts-node typeorm migration:generate -n"
   }
}
